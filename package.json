{
  "name": "@docus/github",
  "version": "1.0.4",
  "description": "GitHub integration for Docus.",
  "keywords": [
    "nuxt",
    "docus",
    "github",
    "module"
  ],
  "repository": "https://github.com/docusgen/github-module",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./runtime": {
      "require": "./dist/runtime.cjs",
      "import": "./dist/runtime.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "runtime.d.ts",
    "runtime.js",
    "runtime.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxt dev example",
    "lint": "eslint --ext .js,.ts,.vue .",
    "test": "yarn lint && jest",
    "clean:example": "rm -rf example/.nuxt example/node_modules",
    "clean:app": "rm -rf src/app/node_modules src/app/.nuxt",
    "clean:dist": "rm -rf dist node_modules",
    "clean": "yarn clean:example && yarn clean:app && yarn clean:dist"
  },
  "resolutions": {
    "mdast-util-from-markdown": "1.0.4"
  },
  "dependencies": {
    "defu": "^5.0.0",
    "object-hash": "^2.2.0",
    "ohmyfetch": "^0.4.7",
    "@docus/remark-github": "^11.1.0",
    "ufo": "^0.7.9",
    "unenv": "^0.4.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-transform-runtime": "latest",
    "@docus/app": "npm:@docus/app-edge@latest",
    "@docus/debug-theme": "^1.2.0",
    "@nuxt/test-utils": "latest",
    "@nuxt/types": "^2.15.8",
    "@nuxt/typescript-runtime": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@types/jest": "^27.0.3",
    "@types/object-hash": "^2.2.1",
    "babel-eslint": "latest",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^3.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.3.1",
    "playwright": "^1.16.3",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.7",
    "unbuild": "^0.5.13"
  },
  "publishConfig": {
    "access": "public"
  }
}
